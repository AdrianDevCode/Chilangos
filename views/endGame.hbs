<div class="main-container">
    <div class="card text-center">
        <div class="card-body">
            {{#if isLoggedIn}}
            <h4>Welcome {{ profile}}</h4>
            <h1>{{answerStatus}}</h1>
            <h1>SCORE {{score}}</h1>
            <a href="/logout" class="btn btn-outline-light hvr-grow hvr-bounce-to-left">Log Out</a>
            {{/if}}
        </div>
    </div>
</div>